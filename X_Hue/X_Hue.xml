<ShaderNodePreset SupportsAdaptiveDegradation="0" LimitInputsToTexture="True" Version="1" Description="
Shift specific hues. Inspired by that 6 vector Color node in Baselight. We have some of this
ability already with the color curves node but this allows you to have tolerance per hue that don't effect
the tolerance of the neighboring hue.

This work is licensed under a Creative Commons Attribution-NonCommercial-ShareAlike 3.0 Unported License.

Matchbox version by miles@fellowla.com" Name="X_Hue">
   <Shader OutputBitDepth="Output" Index="1">
      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="2" Col="0" Page="0" DisplayName="Blue" Type="float" Name="blue">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.5" Inc="0.01" Tooltip="" Row="2" Col="1" Page="0" DisplayName="B Tolerance" Type="float" Name="blue_s">
      </Uniform>

      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="0" Col="2" Page="0" DisplayName="Cyan" Type="float" Name="cyan">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.5" Inc="0.01" Tooltip="" Row="0" Col="3" Page="0" DisplayName="C Tolerance" Type="float" Name="cyan_s">
      </Uniform>

      <Uniform Index="0" NoInput="Error" Tooltip="" DisplayName="front" InputType="Front" Mipmaps="False" GL_TEXTURE_WRAP_T="GL_REPEAT" GL_TEXTURE_WRAP_S="GL_REPEAT" GL_TEXTURE_MAG_FILTER="GL_LINEAR" GL_TEXTURE_MIN_FILTER="GL_LINEAR" Type="sampler2D" Name="front">
      </Uniform>
      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="1" Col="0" Page="0" DisplayName="Green" Type="float" Name="green">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.5" Inc="0.01" Tooltip="" Row="1" Col="1" Page="0" DisplayName="G Tolerance" Type="float" Name="green_s">
      </Uniform>

      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="1" Col="2" Page="0" DisplayName="Magenta" Type="float" Name="magenta">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.5" Inc="0.01" Tooltip="" Row="1" Col="3" Page="0" DisplayName="M Tolerence" Type="float" Name="magenta_s">
      </Uniform>

 	  <Uniform Row="4" Col="3" Width="25" Page="0" Default="False" Tooltip="" DisplayName="Matte Output" Type="int" Name="matte_output" ValueType="Popup">
            <PopupEntry Title="Red" Value="0">
            </PopupEntry>
            <PopupEntry Title="Green" Value="1">
            </PopupEntry>
            <PopupEntry Title="Blue" Value="2">
            </PopupEntry>
            <PopupEntry Title="Cyan" Value="3">
            </PopupEntry>
            <PopupEntry Title="Magenta" Value="4">
            </PopupEntry>
            <PopupEntry Title="Yellow" Value="5">
            </PopupEntry>
        </Uniform>
      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="0" Col="0" Page="0" DisplayName="Red" Type="float" Name="red">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.5" Inc="0.01" Tooltip="" Row="0" Col="1" Page="0" DisplayName="R Tolerance" Type="float" Name="red_s">
      </Uniform>

      <Uniform ResDependent="None" Max="360.0" Min="-360.0" Default="0.0" Inc="1.0" Tooltip="" Row="2" Col="2" Page="0" DisplayName="Yellow" Type="float" Name="yellow">
      </Uniform>
      <Uniform ResDependent="None" Max="10.0" Min="-10.0" Default="0.0" Inc="0.01" Tooltip="" Row="2" Col="3" Page="0" DisplayName="Y Tolerence" Type="float" Name="yellow_s">
      </Uniform>
   </Shader>
   <Page Name="Page 1" Page="0">
      <Col Name="RGB Rotation" Col="0" Page="0">
      </Col>
      <Col Name="" Col="1" Page="0">
      </Col>
      <Col Name="CMY Rotation" Col="2" Page="0">
      </Col>
      <Col Name="" Col="3" Page="0">
      </Col>
   </Page>
</ShaderNodePreset>
